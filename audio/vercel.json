{
  "headers": [
    {
      "source": "/audio/(.*)\\.wav",
      "headers": [
        {
          "key": "Content-Type",
          "value": "audio/wav"
        },
        {
          "key": "Content-Disposition",
          "value": "inline"
        }
      ]
    },
    {
      "source": "/audio/(.*)\\.mp3",
      "headers": [
        {
          "key": "Content-Type",
          "value": "audio/mpeg"
        },
        {
          "key": "Content-Disposition",
          "value": "inline"
        }
      ]
    }
  ]
}
